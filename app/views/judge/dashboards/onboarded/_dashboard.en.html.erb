<div class="relative">
  <% if SeasonToggles.judging_off? %>
    <%= render 'judge/dashboards/onboarded/rebrand/judging_off' %>
  <% elsif SeasonToggles.quarterfinals_judging? %>
    <% if current_judge.live_event? %>
      <%= render 'judge/dashboards/onboarded/rebrand/live_events' %>
    <% else %>
      <%= render 'judge/dashboards/onboarded/rebrand/quarterfinals_or_semifinals' %>
    <% end %>
  <% elsif SeasonToggles.between_rounds? %>
    <%= render 'judge/dashboards/onboarded/rebrand/between_rounds' %>
  <% elsif SeasonToggles.semifinals_judging? %>
    <%= render 'judge/dashboards/onboarded/rebrand/quarterfinals_or_semifinals' %>
  <% elsif SeasonToggles.judging_finished? %>
    <% if SeasonToggles.display_scores_and_certs? %>
      <%= render 'judge/dashboards/onboarded/rebrand/judging_finished_certs_on' %>
    <% else %>
      <%= render 'judge/dashboards/onboarded/rebrand/judging_finished_certs_off' %>
    <% end %>
  <% end %>
</div>
