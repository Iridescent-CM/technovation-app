<%
  url ||= send("#{current_scope}_profile_path")
  admin ||= false
  form = (current_profile.rebranded? || current_profile.is_a?(MentorProfile)) ?
    'signups/rebranded/form' :
    'signups/form'
%>

<%= render layout: form,
  locals: {
    signup: profile,
    url: url,
    title: title,
    admin: admin,
  } do |f| %>
<% end %>
