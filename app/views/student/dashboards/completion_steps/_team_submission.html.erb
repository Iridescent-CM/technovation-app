<% if current_team.present? and current_student.onboarded? %>
  <% if current_team.submission.present? %>
    <%= render(
      "student/dashboards/completion_steps/team_submission_progress",
      submission: current_team.submission
    ) %>
  <% else %>
    <%= render(
      "student/dashboards/completion_steps/start_team_submission"
    ) %>
  <% end %>
<% end %>