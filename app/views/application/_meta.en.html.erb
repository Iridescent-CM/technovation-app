<meta charset="utf-8">
<meta http-equiv="x-ua-compatible" content="ie=edge">

<meta
  name="description"
  content="Join 20,000+ girls around the world who are solving real problems using technology. Learn how to code, market, and pitch a mobile app and business with Technovation Girls."
/>

<meta
  name="viewport"
  content="width=device-width, initial-scale=1, shrink-to-fit=no"
/>

<meta property="og:url" content="https://my.technovationchallenge.org" />
<meta property="og:type" content="website" />

<meta
  property="og:title"
  content="<%= content_for?(:title) ? yield(:title) : t("views.application.site_title") %>"
/>

<meta
  property="og:description"
  content="Join 20,000+ girls who are changing the world using technology."
/>

<meta
  property="og:image"
  content="<%= asset_url "social-sharing-preview-image.png" %>"
/>

<meta
  data-current-profile-id="<%= current_profile.id %>"
  data-current-profile-type="<%= current_profile_type %>"
/>

